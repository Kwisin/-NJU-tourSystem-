<!--pages/personal_center/personal_activity/personal_activity.wxml-->
<view class="page" >
  <view class="page__bd">
          <view class="weui-panel weui-panel_access" wx:for="{{myactivity}}" wx:key="id" wx:for-item="item">
            <view class="weui-panel__hd">{{item.createdTime}} ～ {{item.deadline}}</view>
            <view class="weui-panel__bd">
                <!-- <跳转到活动详情页面> -->
                <navigator url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
                    <!-- <view class="weui-media-box__hd weui-media-box__hd_in-appmsg"> -->
                        <!-- <image class="weui-media-box__thumb" src="{{icon60}}" /> -->
                    <!-- </view> -->
                    <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                        <view class="weui-media-box__title">{{item.name}}</view>
                        <view class="weui-media-box__desc">{{item.description}}</view>
                    </view>
                </navigator>
               
            </view>
            <navigator url="/pages/personal_center/personal_activity/check/check?aid={{item.id}}">
            <view class="weui-panel__ft">
                <view class="weui-cell weui-cell_access weui-cell_link">
                    <view class="weui-cell__bd">查看更多</view>
                    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                </view>
            </view>
                <!-- <button class="weui-btn mini-btn" type="primary" plain="true" size="mini" bindtap="agree_sub">同意</button>
                <button class="weui-btn mini-btn" type="warn"  plain="true" size="mini" bindtap="refuse_sub">拒绝</button> -->
             
             </navigator>
        </view>


        
  </view>
</view>