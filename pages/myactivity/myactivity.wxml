<!--pages/myactivity/myactivity.wxml-->
<view class="page">
    <view class="firrow">
        <button class="join"  plain='true' bindtap="clickjoin">未开始的活动</button>
    </view>
    <view class="secrow">
        <button class="apply" plain='true' bindtap="clickapply">正进行的活动</button>
        <button class="exa" plain='true' bindtap="clickexa">已结束的活动</button>
    </view>
</view>