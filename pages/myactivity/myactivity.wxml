<!--pages/myactivity/myactivity.wxml-->
<view class="page">
    <view class="firrow">
    <button class="button_class1"   bindtap="clicknew"><image src='../../img/timg.png' style="height:135px;width:100%" mode='widthFix'></image>未开始的活动</button>
    
        
    </view>
    <view class="firrow">
        <button class="button_class2"  bindtap="clickongoing"><image src='../../img/test.png' style="height:135px;width:100%" ></image>进行中的活动</button>
        </view>
    <view class="firrow">
        <button class="button_class3"  bindtap="clickfinished"><image src='../../img/timg2.png' style="height:135px;width:100%" mode='widthFix'></image>已结束的活动</button>
    </view>
</view>